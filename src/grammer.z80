sqrtHL_loop:
	sbc	hl,de
	jr	nc,+_
	add	hl,de
_:
	ccf
	rl	d
	rla
	adc	hl,hl
	rla
	adc	hl,hl
	djnz	sqrtHL_loop
  ld e,h
  ld l,d
  ld h,b
  ld d,b
