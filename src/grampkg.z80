  .db "Gram"
  .dw 0
  .dw (+_-2-$)/2
  .dw p_Disp-P_Disp
  .dw p_Text-P_Disp
  .dw p_Menu-P_Disp
_:
p_Disp: .db "DISP",$29 \ .dw p_Text-$-2
