    jp cmdJmp
p_Text: .db "TEXT",$10 \ .dw p_menu-$-2
    ld a,$93
    jp cmdJmp
p_menu:
  .db $E6,0 \ .dw p_end-$-2
Menu_code_start:
  ld hl,(TempWord1)
  ld (parsePtr),hl
#include "cmd/menu.z80"
p_end:
